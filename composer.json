    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-fileinfo": "*",
        "sentry/sdk": "^3.5"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
